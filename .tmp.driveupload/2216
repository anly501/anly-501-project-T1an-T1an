   "outputs": [],
   "source": [
    "import random\n",
    "def generate_label_data(class_labels, weights,N=10000):\n",
    "    y=random.choices(class_labels, weights = weights, k = N)\n",
    "    return y"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "b0b69fca",
   "metadata": {},
   "outputs": [],
   "source": [
    "all_len=len(y_test)\n",
    "len_0=len(y_test[y_test==0])\n",
    "len_1=len(y_test[y_test==1])\n",
    "pro_label_y_0=len_0/all_len\n",
    "pro_label_y_1=len_1/all_len\n",
    "y=generate_label_data([0,1],[pro_label_y_0,pro_label_y_1],all_len)\n",
    "y_pred = y\n",
    "y_true = y_test"
   ]
